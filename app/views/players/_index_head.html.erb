<thead>
  <% age = System::SEASON - season %>

  <tr class="under-age">
    <th colspan="15">U<%= age %> (Season <%= season %>)</th>
  </tr>

  <tr class="head">
    <th>N</th>
    <th>Name</th>
    <th>Position</th>
    <th>Sell</th>
    <% if age >= 16 %><th>T<%= System::SEASON %></th><% end %>
    <% if age >= 17 %><th>T<%= System::SEASON - 1 %></th><% end %>
    <% if age >= 18 %><th>T<%= System::SEASON - 2 %></th><% end %>
    <% if age >= 19 %><th>T<%= System::SEASON - 3 %></th><% end %>
    <% if age >= 20 %><th>T<%= System::SEASON - 4 %></th><% end %>
    <% if age >= 21 %><th>T<%= System::SEASON - 5 %></th><% end %>
    <% if age >= 22 %><th>T<%= System::SEASON - 6 %></th><% end %>
    <% if age >= 23 %><th>T<%= System::SEASON - 7 %></th><% end %>
    <th colspan="2"></th>
  </tr>
</thead>
